<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.a11y.orca.profile">
    <key name="profile-version" type="i">
      <default>-1</default>
      <summary>The version for the profile.</summary>
    </key>
    <key name="application-settings" type="as">
      <default>[]</default>
      <summary>The list of applications with specific settings.</summary>
    </key>
    <key name="active-plugins" type="as">
      <default>['ByeOrca','Clipboard','HelloOrca','Date','Time','MouseReview']</default>
      <summary>The list enabled plugins</summary>
    </key>
  </schema>
</schemalist>
